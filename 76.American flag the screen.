#include<iostream>
using namespace std;
int main() {
    for(int i = 0; i < 10; i++) {
        for(int j = 0; j < 20; j++) {
            if(i < 2) cout << "\x1B[31m*\x1B[0m"; 
            else if(i < 6 && j < 10) cout << "\x1B[34m*\x1B[0m";
            else cout << "\x1B[37m*\x1B[0m"; 
        }
        cout << endl;
    }
    return 0;
}
